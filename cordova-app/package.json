{
  "name": "uk.geovation.photos",
  "version": "1.1.0",
  "displayName": "Geovation Photos",
  "homepage": "./",
  "cordova": {
    "platforms": [
      "browser",
      "ios"
    ],
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-splashscreen": {},
      "cordova-ios-plugin-no-export-compliance": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-google-analytics": {
        "GMS_VERSION": "11.0.1"
      },
      "cordova-plugin-camera-with-exif": {
        "ANDROID_SUPPORT_V4_VERSION": "27.+",
        "CAMERA_USAGE_DESCRIPTION": "Plastic Patrol needs camera access to take pictures of the plastic that you collect.",
        "PHOTOLIBRARY_USAGE_DESCRIPTION": "Plastic Patrol needs photo library access to get the picture of the plastic that you collect."
      },
      "cordova-plugin-geolocation": {}
    }
  },
  "dependencies": {
    "cordova-browser": "^5.0.4",
    "cordova-ios": "^5.0.0",
    "cordova-ios-plugin-no-export-compliance": "0.0.5",
    "cordova-plugin-camera-with-exif": "^1.2.5",
    "cordova-plugin-geolocation": "^4.0.1",
    "cordova-plugin-google-analytics": "^1.8.6",
    "cordova-plugin-splashscreen": "^5.0.2",
    "cordova-plugin-statusbar": "^2.4.2",
    "cordova-plugin-whitelist": "^1.3.3"
  },
  "devDependencies": {
    "http-server": "^0.11.1",
    "xmldom": "^0.1.27"
  }
}