{
  "name": "uk.geovation.photos",
  "version": "1.1.0",
  "displayName": "Geovation Photos",
  "homepage": "./",
  "cordova": {
    "platforms": [
      "android",
      "ios"
    ],
    "plugins": {
      "cordova-plugin-camera-with-exif": {
        "ANDROID_SUPPORT_V4_VERSION": "27.+"
      },
      "cordova-plugin-geolocation": {
        "ANDROID_SUPPORT_V4_VERSION": "27.+"
      },
      "cordova-plugin-google-analytics": {
        "ANDROID_SUPPORT_V4_VERSION": "27.+",
        "GMS_VERSION": "11.0.1"
      },
      "cordova-plugin-splashscreen": {
        "ANDROID_SUPPORT_V4_VERSION": "27.+",
        "GMS_VERSION": "11.0.1"
      },
      "cordova-plugin-statusbar": {
        "ANDROID_SUPPORT_V4_VERSION": "27.+",
        "GMS_VERSION": "11.0.1"
      },
      "cordova-plugin-whitelist": {
        "ANDROID_SUPPORT_V4_VERSION": "27.+",
        "GMS_VERSION": "11.0.1"
      },
      "cordova-ios-plugin-no-export-compliance": {
        "ANDROID_SUPPORT_V4_VERSION": "27.+",
        "GMS_VERSION": "11.0.1"
      }
    }
  },
  "dependencies": {
    "cordova-android": "^8.0.0",
    "cordova-ios": "^5.0.1",
    "cordova-ios-plugin-no-export-compliance": "0.0.5",
    "cordova-plugin-camera-with-exif": "^1.2.7",
    "cordova-plugin-geolocation": "^4.0.1",
    "cordova-plugin-google-analytics": "^1.8.6",
    "cordova-plugin-splashscreen": "^5.0.3",
    "cordova-plugin-statusbar": "^2.4.3",
    "cordova-plugin-whitelist": "^1.3.4"
  },
  "devDependencies": {
    "http-server": "^0.11.1",
    "xmldom": "^0.1.27"
  }
}
